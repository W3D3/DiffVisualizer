  "scripts": {
    "lint": "eslint src/*.js",
    "build": "webpack",
    "watch:build": "webpack --watch",
    "watch:server": "nodemon server/index.js",
    "start": "node server/index.js",
    "dev": "npm-run-all --parallel watch:server watch:build",
    "test": "npm-run-all test:server test:start",
    "test:start": "protractor test/e2e/conf.js",
    "test:server": "webdriver-manager start --detach",
    "prestart": "npm run build",
    "prebuild": "npm run unittest",
    "pretest": "webdriver-manager update",
    "posttest": "webdriver-manager shutdown",
    "unittest": "./node_modules/.bin/mocha --compilers js:babel-core/register test/*-test.js",
    "preunittest": "npm run lint",
    "coverage": "./node_modules/.bin/nyc --reporter=html --reporter=text ./node_modules/mocha/bin/_mocha --compilers js:babel-core/register test/*-test.js",
    "docker-build": "docker-build",
    "docker-run": "docker-run",
    "docker-push": "docker-push",
    "deploy": "docker-build && docker-push"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
